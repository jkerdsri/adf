{
	"name": "Snowflake_to_Salesforce",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SnowflakeSource",
						"exportSettings": {
							"type": "SnowflakeExportCopyCommand"
						}
					},
					"sink": {
						"type": "SalesforceSink",
						"writeBatchSize": 100000,
						"writeBehavior": "upsert",
						"externalIdFieldName": "Name",
						"ignoreNullValues": false
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "AzureBlobStorage1",
							"type": "LinkedServiceReference"
						},
						"path": "adf-blob"
					}
				},
				"inputs": [
					{
						"referenceName": "SnowflakeTable1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SalesforceObject1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}